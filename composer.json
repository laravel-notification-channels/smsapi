{
    "name": "laravel-notification-channels/smsapi",
    "description": "Smsapi Notifications Channel for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/smsapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateusz Drost",
            "email": "mat.drost@gmail.com",
            "homepage": "https://github.com/mdrost",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0 || ~11.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0 || ~11.0",
        "smsapi/php-client": "^1.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Smsapi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Smsapi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Smsapi\\SmsapiServiceProvider"
            ]
        }
    }
}
